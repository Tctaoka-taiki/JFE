    <!-- LOGMODE 0:無 1:有 -->
    <add key="LOGMODE" value="1" />

    <!-- 時間表示設定 -->
    <add key="TimeFormat" value="yyyy/MM/dd HH:mm:ss" />
    <add key="TimeInterval" value="1000" />

    <!-- スレッド設定 -->
    <add key="TimeInterval_7" value="1000" />
    <add key="TimeInterval_8" value="1000" />
    <add key="TimeInterval_9" value="1000" />
    <add key="IniWaitInterval_7" value="5000" />
    <add key="IniWaitInterval_8" value="5000" />
    <add key="IniWaitInterval_9" value="5000" />

    <!-- ファイル設定 -->
    <add key="filepath_計測_7" value="C:\XYZ-DATA\" />
    <add key="filepath_履歴_7" value="\log\" />
    <add key="filepath_計測_8" value="C:\XYZ-DATA\" />
    <add key="filepath_履歴_8" value="\log\" />
    <add key="filepath_計測_9" value="C:\XYZ-DATA\" />
